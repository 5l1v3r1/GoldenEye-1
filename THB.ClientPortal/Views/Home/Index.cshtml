@{
    ViewBag.Title = "Home Page";
}
<script type="text/javascript" src="~/Scripts/jquery-2.1.4.js"></script>
<script type="text/javascript" src="~/Scripts/knockout-3.3.0.js"></script>
<script type="text/javascript" src="~/Scripts/knockout.mapping-latest.js"></script>
<script type="text/javascript" src="~/Scripts/sammy-0.7.5.js"></script>
<script type="text/javascript" src="~/Scripts/app/SammyRoutes.js"></script>

<script type="text/javascript" src="~/Scripts/knockstrap.js"></script>
<script type="text/javascript" src="~/Scripts/moment.js"></script>
<script src="~/Scripts/app/common.js"></script>

<script src="~/Scripts/app/Core/Loaders/TemplateFromUrlLoader.js"></script>
<script src="~/Scripts/app/Components/Login/LoginViewModel.js"></script>
<script src="~/Scripts/app/ViewModels/TopMenuViewModel.js"></script>
<script src="~/Scripts/app/ViewModels/AddTaskViewModel.js"></script>
<script src="~/Scripts/app/ViewModels/DetailViewModel.js"></script>
<script src="~/Scripts/app/ViewModels/TaskListViewModel.js"></script>
<script src="~/Scripts/app/ViewModels/MainViewModel.js"></script>
<script src="~/Scripts/app/ViewModelLocator.js"></script>
<script src="~/Scripts/app/app.js"></script>
<script type="text/javascript" src="~/Scripts/app/RestService.js"></script>

<link rel="stylesheet" type="text/css" href="~/Content/misc.css">


<template id="main-view">
    <div data-bind="component: { name: app.current, params: app.params }"></div>
</template>

<div class="container">
    <div class="row">
        <div class="span12">
            <div data-bind="component: 'main'"></div>
        </div>
    </div>
</div>

<template id="Zlecenia">

    <h2>Zlecenia</h2>
    <br />

    @*<div class="table table-bordered" data-bind="with: TaskListViewModel">
        <div class="row">
            <div class="col-md-1">
                <i class="glyphicon glyphicon-sort-by-order"></i> Numer
            </div>
            <div class="col-md-4">
                <i class="glyphicon glyphicon-sort-by-alphabet"></i> Nazwa
            </div>
            <div class="col-md-3">
                <i class="glyphicon glyphicon-calendar"></i> Data
            </div>
            <div class="col-md-2">
                <i class="glyphicon glyphicon-tasks"></i> Stopień realizacji
            </div>
            <div class="col-md-1">
            </div>
        </div>
        <div class="row" data-bind="foreach: tasklist">
            <div class="col-md-1">
                <span data-bind='text: Number, uniqueName: true' />
            </div>
            <div class="col-md-4">
                <span data-bind='text: TaskName' />
            </div>
            <div class="col-md-3">
                <span data-bind="text: moment(Date()).format('DD.MM.YYYY')" />
            </div>
            <div class="col-md-2">
                <span data-bind='text: Progress' /> 
            </div>
            <div class="col-md-1">
                <a data-bind="attr: { href: '#Detale/' + Id() }">Detale</a>
             </div>
         </div>
    </div>*@

    <div class="grid">
        <table data-bind='with: TaskListViewModel' class="table table-bordered" cellspacing="0">
            <thead>
                <tr class="grid-header">
                    <th><i class="glyphicon glyphicon-sort-by-order"></i> Numer</th>
                    <th><i class="glyphicon glyphicon-sort-by-alphabet"></i> Nazwa</th>
                    <th><i class="glyphicon glyphicon-calendar"></i> Data</th>
                    <th><i class="glyphicon glyphicon-tasks"></i> Stopień realizacji</th>
                    <th></th>
                </tr>
            </thead>
            <tbody data-bind='foreach: tasklist'>
                <tr>
                    <td><span data-bind='text: Number, uniqueName: true'/></td>
                    <td><span data-bind='text: TaskName'/></td>
                    <td><span data-bind="text: moment(Date()).format('DD.MM.YYYY')"/></td>
                    <td><div data-bind="progress: { value: Progress, textHidden: false }" class="customprogress"></div></td>
                    <td><a data-bind="attr: { href: '#Detale/' + Id() }">Detale</a></td>
                </tr>
            </tbody>
        </table>
    </div>


</template>

<template id="Dodaj">
    <h2>Dodaj zlecenie</h2>

    <form data-bind="submit: addTask" class="form-horizontal">
        <div class="form-group">
            <label for="TaskName" class="col-sm-2 control-label">Nazwa zlecenia</label>
            <div class="col-sm-10">
                <input type="text" data-bind="value: TaskName" class="form-control" id="TaskName" required />
            </div>
        </div>
        <div class="form-group">
            <label for="Number" class="col-sm-2 control-label">Numer zlecenia</label>
            <div class="col-sm-10">
                <input type="number" data-bind="value: Number" class="form-control" id="Number" required />
            </div>
        </div>
        <div class="form-group">
            <label for="Progress" class="col-sm-2 control-label">Stopień realizacji</label>
            <div class="col-sm-10">
                <input type="number" data-bind="value: Progress" class="form-control" id="Progress" required />
            </div>
        </div>
        @*        <div class="form-group">
        <label for="Type" class="col-sm-2 control-label">Rodzaj/przedmiot</label>
        <div class="col-sm-10">
            <select data-bind="options: Type" class="form-control" id="Type"></select>
        </div>
    </div>*@

        <div class="form-group">
            <label for="Date" class="col-sm-2 control-label">Data przyjęcia</label>
            <div class="col-sm-10">
                <input type="date" data-bind="value: Date" class="form-control" id="Date">
            </div>
        </div>
        <div class="form-group">
            <label for="Amount" class="col-sm-2 control-label">Ilość</label>
            <div class="col-sm-10">
                <input type="number" data-bind="value: Amount" class="form-control" id="Amount">
            </div>
        </div>
        <div class="form-group">
            <label for="Time" class="col-sm-2 control-label">Planowany czas trwania</label>
            <div class="col-sm-10">
                <input type="number" data-bind="value: Time" class="form-control" id="Time">
            </div>
        </div>
        <div class="form-group">
            <label for="StartDate" class="col-sm-2 control-label">Planowana data rozpoczęcia</label>
            <div class="col-sm-10">
                <input type="date" data-bind="value: StartDate" class="form-control" id="StartDate">
            </div>
        </div>
        <div class="form-group">
            <label for="EndDate" class="col-sm-2 control-label">Planowana data zakończenia</label>
            <div class="col-sm-10">
                <input type="date" data-bind="value: EndDate" class="form-control" id="EndDate">
            </div>
        </div>
        <div class="form-group">
            <label for="PlanDate" class="col-sm-2 control-label">Data zaplanowania</label>
            <div class="col-sm-10">
                <input type="date" data-bind="value: PlanDate" class="form-control" id="PlanDate" />
            </div>
        </div>
        <div class="form-group">
            <label for="Description" class="col-sm-2 control-label">Opis</label>
            <div class="col-sm-10">
                <textarea rows="5" data-bind="value: Description" id="Description" class="form-control"></textarea>
            </div>
        </div>
        <div class="form-group">
            <label for="Color" class="col-sm-2 control-label">Kolor</label>
            <div class="col-sm-10">
                <input type="color" data-bind="value: Color" class="form-control" id="Color">
            </div>
        </div>
        <div class="form-group">
            <div class="col-sm-offset-2 col-sm-10">
                <div class="checkbox">
                    <label>
                        <input type="checkbox" data-bind="value: IsInternal"> Czy wewnętrzne?
                    </label>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="col-sm-offset-2 col-sm-10">
                <button type="submit" class="btn btn-default">Dodaj</button>
            </div>
        </div>
    </form>

</template>


<template id="Detale">
    <div class="container">
        <div class="page-header">
            <h2>Szczegóły zlecenia</h2>
        </div>
        <div class="details" data-bind="visible: getTask">
            <p><label>Nazwa zlecenia:</label> <span data-bind='text: TaskName'></span></p>
            <p><label>Numer:</label> <span data-bind='text: Number'></span></p>
            <p><label>Data przyjęcia:</label> <span data-bind="text: moment(Date()).format('DD.MM.YYYY')"></span></p>
            <p><label>Rodzaj/przedmiot: </label> <span data-bind='text: Type'></span></p>
            <p><label>Ilość: </label> <span data-bind='text: Amount'></span></p>
            <p><label>Planowany czas trwania: </label> <span data-bind='text: Time'></span></p>
            <p><label>Planowana data rozpoczęcia: </label> <span data-bind='text: StartDate'></span></p>
            <p><label>Planowana data zakończenia: </label> <span data-bind='text: EndDate'></span></p>
            <p><label>Data zaplanowania: </label> <span data-bind='text: PlanDate'></span></p>
            <p><label>Opis: </label> <span data-bind='text: Description'></span></p>
            <p><label>Stopień realizacji: </label> <span data-bind='text: Progress'></span>%</p>
        </div>
    </div>

</template>

